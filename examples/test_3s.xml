                    </siteModel>
                    <branchRateModel id="strictClock:gene1" clock.rate="@clockRate:gene" spec="StrictClockModel"/>
                </distribution>
                <distribution id="likelihood:gene2" data="@gene2" tree="@tree:gene2" spec="TreeLikelihood">
